    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "svelte-sonner": "^1.0.5",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.0.0",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "vaul-svelte": "^1.0.0-next.7",
    "vite": "^7.0.4",
    "vitest": "^3.2.3",
    "vitest-browser-svelte": "^0.1.0",
    "zod": "^3.25.76"
  },
